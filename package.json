{
  "name": "gitlab-ci-local",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "version": "4.8.1",
  "scripts": {
    "prepublishOnly": "npm run check-all && chmod +x dist/index.js",
    "pkg-linux": "pkg dist/index.js -t node14-linux-x64 -o bin/linux/gitlab-ci-local && chmod +x bin/linux/gitlab-ci-local && gzip -c bin/linux/gitlab-ci-local > bin/linux.gz",
    "pkg-macos": "pkg dist/index.js -t node14-macos-x64 -o bin/macos/gitlab-ci-local && gzip -c bin/macos/gitlab-ci-local > bin/macos.gz",
    "pkg-win": "pkg dist/index.js -t node14-win-x64 -o bin/win/gitlab-ci-local && gzip -c bin/win/gitlab-ci-local.exe > bin/win.gz",
    "pkg-all": "npm run pkg-linux && npm run pkg-macos && npm run pkg-win",
    "pkg": "pkg",
    "build": "tsc",
    "check-all": "npm run build && npm run lint && npm run ncu && npm run test:badges && npm audit --parseable",
    "lint": "tslint --project .",
    "ncu": "ncu --target latest -e 2",
    "test": "jest",
    "test:badges": "jest --coverage && make-coverage-badge",
    "start": "node -r source-map-support/register dist/index.js --cwd examples/docker-compose-nodejs"
  },
  "dependencies": {
    "ansi-colors": "^4.1.1",
    "clone": "^2.1.2",
    "dateformat": "^4.4.1",
    "deep-extend": "^0.6.0",
    "fs-extra": "^9.0.1",
    "js-yaml": "^4.0.0",
    "pretty-hrtime": "^1.0.3",
    "yargs": "^16.2.0"
  },
  "pkg": {
    "targets": [
      "node14"
    ]
  },
  "author": "Mads Jon Nielsen <madsjon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/clone": "^2.1.0",
    "@types/deep-extend": "^0.4.31",
    "@types/fs-extra": "^9.0.6",
    "@types/js-yaml": "^4.0.0",
    "@types/mock-fs": "^4.13.0",
    "@types/pretty-hrtime": "^1.0.0",
    "@types/shelljs": "^0.8.8",
    "@types/yaml": "^1.9.7",
    "@types/yargs": "^15.0.12",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "jest": "^26.6.3",
    "jest-mock-process": "^1.4.0",
    "make-coverage-badge": "^1.2.0",
    "mock-fs": "^4.13.0",
    "npm-check-updates": "^10.2.5",
    "pkg": "^4.4.9",
    "source-map-support": "^0.5.19",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
